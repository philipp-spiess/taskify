<h1>Listing users</h1>

<table class="table table-bordered">
  <tr>
    <th>Nickname</th>
    <th>Name</th>
    <th>Isadmin</th>
    <th>Email</th>
    <th>Password</th>
    <th>Location</th>
    <th>Website</th>
    <th>Company</th>
    <th>About</th>
    <th>Css</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.nickname %></td>
    <td><%= user.name %></td>
    <td><%= user.isAdmin %></td>
    <td><%= user.email %></td>
    <td><%= user.password %></td>
    <td><%= user.location %></td>
    <td><%= user.website %></td>
    <td><%= user.company %></td>
    <td><%= user.about %></td>
    <td><%= user.css %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
